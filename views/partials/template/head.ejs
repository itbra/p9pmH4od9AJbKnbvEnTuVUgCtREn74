<!-- Test framework (Mocha, etc) and style overrides -->
<% if (test) { %>
  <link rel="stylesheet" href="/vendor/mocha.css">
    <style>
        /* Fix override by TWBS. They use a .progress too that breaks mochas style */
        #mocha {
           margin: 0;
        }
        #mocha .progress {
           height: 40px;
           background: none;
           border-radius: 0;
           box-shadow: none;
        }
        #mocha-report .suite {
           margin-left: 0
        }
    </style>
<% } %>

